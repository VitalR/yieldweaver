# ----------------------------
# Network (Tenderly VNet)
# ----------------------------
# HTTP RPC for your Tenderly Virtual Mainnet fork (VNet)
ETH_RPC_URL=https://virtual.mainnet.eu.rpc.tenderly.co/82c86106-662e-4d7f-a974-c311987358ff

# ----------------------------
# Deployer
# ----------------------------
# Hex private key for the deployer (no 0x? -> add it)
DEPLOYER_PRIVATE_KEY=0xYOUR_PRIVATE_KEY

# ----------------------------
# Target Spark Savings Vault V2 (ERC-4626) + Underlying
# ----------------------------
# Example: spUSDC on mainnet state mirrored into VNet
SPARK_VAULT=0x28B3a8fb53B741A8Fd78c0fb9A6B2393d896a43d
# Underlying ERC-20 that vault.asset() returns (USDC on mainnet)
UNDERLYING=0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48

# ----------------------------
# Strategy metadata & roles
# ----------------------------
NAME="Spark USDC YDS Tokenized"
SYMBOL="SP-USDC-YDS"
MANAGEMENT=0xYourEOAOrMultisig
KEEPER=0xYourEOAOrBot
EMERGENCY_ADMIN=0xYourEOAOrMultisig
DONATION=0xDragonRouterOrReceiver

# Enable donation-share burning to cover losses (true/false)
ENABLE_BURNING=true

# Spark referral code (uint16). Use 0 to disable.
REFERRAL=0

# ----------------------------
# (Optional) Reuse an existing factory
# If unset or zero, script deploys a new factory owned by DEPLOYER
# ----------------------------
# FACTORY=0xExistingFactoryAddress

# ----------------------------
# RunSparkYDSMainFlow
# ----------------------------
# set env (examples)
STRATEGY=0xDeployedStrategy
TOKENIZED=0xDeployedTokenized

# optional knobs:
USER=0xYourEOAO         # defaults to deployer
AMOUNT=100000000        # 100 USDC if 6 dec
DO_TEND=true
DO_REPORT=false         # set true later to see non-zero profit
WITHDRAW_BPS=2500       # 25% withdraw (0 to skip)
SLEEP_SECONDS=0
